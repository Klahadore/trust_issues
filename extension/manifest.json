{
    "name" : "Trust Issues",
    "description" : "reads terms and conditions to ensure safe policy",
    "version": "1.0.0",
    "manifest_version" : 3,
    "permissions": [
        "tabs", "storage","scripting","activeTab", "contextMenus"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background":{
        "scripts": "background.js"
    }
}
